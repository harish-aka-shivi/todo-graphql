  {
  "extends": "airbnb",
  "env": {
    "jest": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "no-restricted-syntax": 0,
    "arrow-parens": ["error", "as-needed"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": [0]
  }
}
